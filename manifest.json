{
	"manifest_version": 2,
	"name": "VK:Music to playlist",
	"version": "0.1.0",
	"description": "Addon for create playlists from posts with audio content",
	"icons": {
		"48": "icons/icon.png"
	},
	"applications": {
		"gecko": {
			"id": "ttywizard@gmail.com",
			"strict_min_version": "45.0"
		}
	},
	"content_scripts": [
		{
			"matches": ["*://*.vk.com/*"],
			"js": ["js/jquery-3.3.1.min.js", "js/main.js"]
		}
	]
}